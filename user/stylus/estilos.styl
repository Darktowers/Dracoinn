@import "../stylus/normalize.styl"
@import "nib"
@font-face 
	font-family 'icomoon'
	src url('../fonts/icomoon.eot?sqehjs')
	src url('../fonts/icomoon.eot?#iefixsqehjs') format('embedded-opentype'),
		url('../fonts/icomoon.woff?sqehjs') format('woff'),
		url('../fonts/icomoon.ttf?sqehjs') format('truetype'),
		url('../fonts/icomoon.svg?sqehjs#icomoon') format('svg')
	font-weight normal
	font-style normal


[class^="icon-"], [class*=" icon-"] 
	font-family 'icomoon'
	speak none
	font-style normal
	font-weight normal
	font-variant normal
	text-transform none
	line-height 1

	/* Better Font Rendering =========== */
	-webkit-font-smoothing antialiased
	-moz-osx-font-smoothing grayscale


.icon-plus:before 
	content "\e600"

.icon-envelope:before 
	content "\e60a"

.icon-location:before 
	content "\e601"

.icon-user:before 
	content "\e602"

.icon-search:before 
	content "\e603"

.icon-key:before 
	content "\e60b"

.icon-lock:before 
	content "\e604"

.icon-confi:before 
	content "\e606"

.icon-eye:before 
	content "\e607"

.icon-star:before 
	content "\e609"

.icon-check:before 
	content "\e605"

.icon-close:before 
	content "\e608"